<style>
    #detail_studio_vue{float:left;background:#efefef}
    #detail_studio_vue .header{width:100%;height:78vw;background:#fff;}
    #detail_studio_vue .banner{width:100vw;height:48.98148vw;float:left;position:relative}
    #detail_studio_vue .bannerImg{position:absolute;width:100%;height:100%}
    #detail_barber_vue .geolocation,#detail_studio_vue .geolocation{position:absolute;width:19vw;height:6vw;right:1.2vw;bottom:1.2vw;text-align:center;color:#fff;font-size:3.2vw;line-height:6vw;border-radius:1vw}
    #detail_barber_vue .geolocation>div,#detail_studio_vue .geolocation>div{position:absolute;width:100%;height:100%;background:#000;opacity:.2}
    #detail_barber_vue .geolocation>span,#detail_studio_vue .geolocation>span{z-index:999;position:relative}
    #detail_barber_vue .geolocation>img,#detail_studio_vue .geolocation>img{position:absolute;left:-17%}
    #detail_studio_vue .title{float:left;position:relative;width:100vw;height:14.72222vw;border-bottom:1px solid #ebebeb;border-top:1px solid #ebebeb}
    #detail_studio_vue .tBox{position:absolute;width:27.77778vw;height:9.62963vw;right:2.77778vw;bottom:2.77778vw}
    #detail_studio_vue .tBox .fans{width:50%;height:100%;background:#d60040;text-align:center;float:left;color:#fff;font-size:3.2vw;line-height:3.51852vw;padding-top:1vw}
    #detail_studio_vue .tBox .follow{width:50%;height:100%;background:#e60146;text-align:center;float:left;color:#fff;font-size:3.2vw;line-height:3.51852vw;padding-top:1vw}
    #detail_barber_vue .follow>img,#detail_studio_vue .follow>img{width:15%;position:absolute;left:68%;top:5%}
    #detail_studio_vue .designation{font-size:4.2vw}
    #detail_studio_vue .lBox{position:absolute;}
    #detail_studio_vue .satisfaction{line-height: 4.48148vw;color: #a4a4a4;width: 69vw;height: 14vw;padding: 0 5vw}
    #detail_studio_vue .satisfactionTxt{color:#f60052}
    #detail_studio_vue .promise{position:relative;float:left;width:100%;height:11.38889vw}
    #detail_studio_vue .promiseItem{position:relative;float:left;width:23%;line-height:11vw;font-size:3.2vw;color:#9e9e9e;text-align:center}
    #detail_studio_vue .promiseItem:nth-child(1){margin-left:4%}
    #detail_studio_vue .promiseItem .circle{position:relative;width:4.81481vw;height:4.81481vw;background:url(https://wx-1253594735.cosgz.myqcloud.com/barber/images/icon-check.png) no-repeat;background-size:100% 100%;display:inline-block;top:1.2vw;margin-right:1vw}
    #detail_studio_vue .memberList{float:left;margin-top:3vw;width:100%;position:relative}
    #detail_studio_vue .listItem{position:relative;float:left;width:100%;height:23.5vw;background:#fff;border-bottom:.27778vw solid #f4f4f4}
    #detail_studio_vue .head{width:15.55556vw;height:23.5vw;padding-left:3.7037vw}
    #detail_studio_vue .head img{width:15.55556vw;height:15.55556vw;border-radius:50%}
    #detail_studio_vue .desc{width:42vw;height:23.5vw;padding-left:2.5vw}
    #detail_studio_vue .price{width:15.6vw;color:#e20145;text-align:center;font-size:4.8vw;font-weight:700}
    #detail_studio_vue .price .yuan{position:relative;font-size:70%;top:-3vw}
    #detail_studio_vue .name{font-size:4vw;font-weight:600;line-height:5.2vw}
    #detail_studio_vue .intro{font-size:3.5vw;line-height:5vw}
    #detail_studio_vue .more{font-size:80%;color:#a5a5a5;line-height:4.2vw}
    #detail_studio_vue .cell{position:relative;display:table-cell;vertical-align:middle}
    #detail_studio_vue .status{width:20vw;height:10.74074vw;background:#ddd9d8;line-height:10.74074vw;text-align:center;font-size:3.8vw;font-weight:700;color:#fff;border-radius:1vw}
    #detail_studio_vue .status.on{background:#e60146}
    #detail_studio_vue .detailIntro{width:100%;background:#fff;float:left;margin-top:3.6vw}
    #detail_studio_vue .detailTitle{float:left;height:12.5vw;width:33.33333vw;line-height:12.5vw;text-align:center;color:#ff1f66;font-size:4.4vw;border-bottom:1px solid #e30047}
    #detail_studio_vue .content{float:left;width:100vw;height:25vw;font-size:3.3vw;line-height:5vw}
    #detail_studio_vue .inLine{height:25vw;padding:0 7vw;color:#9d9d9d;text-indent:6.8vw;border-top:1px solid #e5e5e5}
    #detail_studio_vue .bottom{color:#646464;float:left;width:100vw;background:#fff;margin-top:3.6vw;font-size:3.3vw}
    #detail_studio_vue .line{height:11vw}
    #detail_studio_vue .icon{height:15vw;padding-left:4vw}
    #detail_studio_vue .iconL{width:5vw;height:6.48148vw}
    #detail_studio_vue .txtLocation{line-height:4.4vw;width:75.92593vw;height:15vw;padding-left:2vw}
    #detail_studio_vue .cell.check{color:#9e9e9e;font-size:3.8vw;padding-left:1vw}
    #detail_studio_vue .iconZ{width:5vw;height:5vw}
    #detail_studio_vue .line_2 .icon{height:9vw}
    #detail_studio_vue .txtTime{height:9vw;padding-left:2vw}
    #detail_studio_vue .iconD{width:5vw;height:5vw}
    #detail_studio_vue .line_3 .icon{height:9vw}
    #detail_studio_vue .txtTel{height:9vw;padding-left:2vw}
    #detail_studio_vue .txtTel span{font-weight:700;font-size:4vw;color:#979797}
    #latest_comment_vue .comment,#detail_studio_vue .comment{float:left;width:100vw;background:#fff;margin-top:4vw}
    #latest_comment_vue .commentTitle,#detail_studio_vue .commentTitle{float:left;height:12.5vw;width:41.66667vw;line-height:12.5vw;text-align:center;color:#ff1f66;font-size:4.4vw;border-bottom:1px solid #e30047}
    #latest_comment_vue .commentList,#detail_studio_vue .commentList,#detail_barber_vue .commentList{float:left;border-top:1px solid #e8e8e8}
    #latest_comment_vue .commentCheck,#detail_studio_vue .commentCheck{float:left;height:16.94444vw;width:100vw;border-top:1px solid #e8e8e8}
    #latest_comment_vue .commentCheckAll,#detail_studio_vue .commentCheckAll{float:left;width:91.66667vw;height:10.83333vw;line-height:10.83333vw;border:1px solid #e8e8e8;margin:3.05556vw 4.16667vw;text-align:center;color:#9d9d9d;font-size:3.8vw}
    #detail_studio_vue .commentCheckAll:active,#latest_comment_vue .commentCheckAll:active{background:#eee;color:#fff}
    #latest_comment_vue .commentListItem,#detail_studio_vue .commentListItem{float:left;width:100vw}
    #latest_comment_vue .userInfo,#detail_studio_vue .userInfo{margin:2vw auto 3vw auto;width:89.53704vw;height:13.61111vw}
    #latest_comment_vue .userInfo,#detail_studio_vue .userInfo .userHead{float:left;height:13.61111vw;width:13.61111vw;border-radius:50%}
    #latest_comment_vue .userInfo,#detail_studio_vue .userInfo .userInfoBox{float:left;width:37vw;height:100%;margin-left:2.5vw;font-size:3.4vw;padding-top:2vw}
    #latest_comment_vue .userInfo,#detail_studio_vue .userInfo .nameLine{line-height:6vw}
    #latest_comment_vue .userInfo,#detail_studio_vue .userInfo .telLine{line-height:6vw;color:#c5c5c5}
    #latest_comment_vue .userInfo,#detail_studio_vue .userInfo .fa{color:#feb005;font-size:4vw}
    #latest_comment_vue .userInfo,#detail_studio_vue .userInfo .starArea{margin-top:8vw;float:left}
    #latest_comment_vue .userInfo,#detail_studio_vue .userInfo .marks,#detail_barber_vue .commentListItem .userInfo .marks{text-align:right;color:#747474;font-size:3.6vw;margin-top:7vw;margin-left:2.5vw;float:left;width:33vw;}
    #latest_comment_vue .commentDetail,#detail_studio_vue .commentDetail,#detail_barber_vue .commentDetail{word-break:break-all;width:97vw;color:#767676;padding-left:5.8vw;line-height:5.5vw;font-size:3.3vw;display:inline-block}
    #latest_comment_vue .commentDate,#detail_studio_vue .commentDate{color:#c9c9c9;font-size:3.3vw;line-height:9vw;float:right;margin-right:9vw}
    #latest_comment_vue .wokerName,#detail_studio_vue .wokerName{padding-right:3vw}
</style>

<div v-if="studio.id == 20">
    <!--<img style="width: 100%;" src="https://wx-1253594735.file.myqcloud.com/studio/20/temp_20180407.png"/>-->
</div>

<div class="w100 pos_r h100" style="overflow:scroll;-webkit-overflow-scrolling:touch;">
    <div class="header">
        <div class="banner">
            <img :src="studio.img" class="bannerImg">
            <div class="geolocation">
                <div></div>
                <img :src="db.data.oss+'barber/images/location.svg'" class="h100">
                <span>{{studio.distance/1000}}KM</span>
            </div>
        </div>
        <div class="title">
            <div class="lBox">
                <div class="satisfaction flex flex-column" style="justify-content: center">
                    <span style="font-size:5vw;color:#000;font-weight:600">{{studio.name}}</span>
                    <span style="font-size:3vw;">营业时间:{{studio.work_time}} &nbsp;满意度: {{studio.satisfaction}}%</span>
                </div>
            </div>
            <div class="tBox">
                <div class="fans">{{studio.fans}}<br>粉丝数</div>
                <div class="follow" @touchstart="subscribeStudio()">
                    <img :src="followImg"><br>{{(studio.subscribed)?'已关注':'关注'}}
                </div>
            </div>
        </div>
        <table style="height:8.9vh;" class="w100">
            <tr>
                <td style="width:75%;padding:0 20% 0 5%;font-size:3.5vw;color:#a4a4a4;">
                    {{studio.place}} <span style="padding: 0 3%;background: #ddd;border-radius: 20px;">
       {{studio.distance/1000}}KM</span>
                </td>
                <td @touchstart="beHere()" class="txtc" style="font-size:3.5vw;">
                    <img :src="db.data.oss+'barber/images/icon-l.png'" class="iconL w50" style="width: 16%;height: 32%;"><br/>
                    导航&gt;
                </td>
            </tr>
        </table>
        <!--<div class="title" @click="commodity_detail()" style="margin-bottom: -3%;">
            <img style="bottom:10vh"
                 src="https://wx-1253594735.file.myqcloud.com/barber/images/buy_studio_banner_new.gif"
                 width="100%" @click="commodity_detail()"/>
        </div>-->
        <div class="memberList">
            <div class="listItem" v-for="barber in studio.barbers" style="{{barber.last_barber?'background:url(https://wx-1253594735.cosgz.myqcloud.com/barber/images/last_barber_background.png);':''}}">
                <div @click="launcher.detail_barber(barber.id);" class="cell head"><img :src="barber.avt"></div>
                <div @click="launcher.detail_barber(barber.id);" class="cell desc">
                    <div class="name">{{barber.name}}
                        <!--<span style="font-size: 3vw;">{{barber.price > 38 ? '(首席)' : '(资深)'}}</span>-->
                        <img v-if="barber.last_barber" :src="db.data.osss+'barber/images/last_barber_subscribe.png'" style="width: 40%;"/>
                    </div>
                    <div class="intro">{{barber.waitStr}}</div>
                    <!--<div class="more">累计{{barber.comments_count}}条评论,点击查看</div>-->
                </div>
                <div @click="launcher.detail_barber(db.data.detail_studio_index,$index);" class="cell price">{{barber.price}}<i class="yuan">￥</i></div>
                <div class="cell">
                    <!--<div class="status" @click="launcher.quhao(barber,barber.service,barber.work_state)" :style="(barber.work_state==1)?'background:#e60146;':''">{{(barber.work_state==1)?'一键取号':((barber.work_state==0)?'已下班':'吃饭中')}}</div>-->
                    <div class="status" @click="readeyTakeNumber(barber)" :style="(barber.work_state==1)?'background:#e60146;':''">{{(barber.work_state==1)?'一键取号':((barber.work_state==0)?'已下班':'吃饭中')}}</div>
                </div>
            </div>
        </div>
        <div class="comment" v-if="studio.comments>0">
            <div class="commentTitle">用户好评({{studio.comments}})</div>
            <div class="commentList">
                <div class="commentListItem" v-for="comment in studio.lastComment">
                    <div class="userInfo"><img :src="comment.userAvt" class="userHead">
                        <div class="userInfoBox">
                            <div class="nameLine">{{comment.userName}}</div>
                            <div class="telLine">{{comment.userPhone}}</div>
                        </div>
                        <div class="marks">
                            <img v-for="i in [0,1,2,3,4]" :src="db.data.oss+'barber/images/start_orange.svg'" style="width:12%;">
                            5.0分
                        </div>
                    </div> <span class="commentDetail">{{comment.content}}</span>
                    <div class="image" v-if="comment.img.length > 0">
                        <img style="    width: 30%;
    margin-left: 3%;" @click="launcher.look(comment.img.join('|'), src)" v-for="src in comment.img" :src="src">
                    </div>
                    <div class="commentDate"><span>发型师：</span> <span class="wokerName">{{comment.barberName}}</span> <span>{{comment.create_time}}</span></div>
                </div>
            </div>
            <div class="commentCheck" @click="jump('latest_comment');">
                <div class="cell commentCheckAll">查看全部</div>
            </div>
        </div>
        <div class="bottom">
            <div class="cell line">
                <div class="line_3">
                    <div class="cell icon"><img :src="db.data.oss+'barber/images/icon-d.png'" class="iconD"></div>
                    <div class="cell txtTel">电话：<span>{{studio.phone}}</span></div>
                </div>
            </div>
        </div>
    </div>
    <div class="endtouch w100 h100 pos_a t0"></div>
</div>
<div class="bindPhone {{showBindPhoneView? '':'disn'}}">
    <div class="cover" @click="change('showBindPhoneView')"></div>
    <div class="box">
        <div class="line_1">
            {{(!!db.data.user.phone===false)?'绑定手机号':'更换手机号'}}
        </div>
        <div class="line_2">
            <input placeholder="输入手机号" v-model="phone">
        </div>
        <div class="line_3">
            <input placeholder="输入验证码" v-model="phoneCheck">
            <span @click="sendPhoneCheck()">{{checkText}}</span>
        </div>
        <div class="line_4">
            <span @click="bindPhone()">确定</span>
            <span @click="change('showBindPhoneView')">取消</span>
        </div>
    </div>
</div>