B,EAA4BD,GAA5B,EAAgC;AAC5B,gCAAIoB,MAAMH,OAAOjB,CAAP,EAAUqB,IAAV,EAAV;AACA,gCAAG,CAAC,CAACD,GAAL,EAAS;AACLD,qCAAKG,IAAL,CAAUF,GAAV;AACH;AACJ;;AAED,4BAAGD,KAAKlB,MAAL,IAAe,CAAlB,EAAoB;AAChB;AACH,yBAFD,MAEK;AACDsB,+BAAGC,YAAH,CAAgB;AACZC,yCAAS,EADG,EACC;AACbN,sCAAMA,IAFM,CAED;AAFC,6BAAhB;AAIH;AACJ,qBAtDI;AAuDLO,oCAvDK,4BAuDYC,OAvDZ,EAuDoB;AACrBA,gCAAQC,IAAR,CAAa,WAAb,EAA0BC,IAA1B;AACAxC,mCAAW,YAAU;AACjBsC,oCAAQC,IAAR,CAAa,WAAb,EAA0BE,IAA1B;AACH,yBAFD,EAEE,GAFF;AAGH,qBA5DI;AA6DLC,0BA7DK,kBA6DEhC,IA7DF,EA6DO;AACR,4BAAGW,IAAIsB,gBAAP,EAAwB;AACpB,gCAAIC,UAAJ;AACA,gCAAIC,QAAJ;AACAD,yCAAaE,YAAY,YAAU;AAC/B,oCAAG,CAACzB,IAAIsB,gBAAR,EAAyB;AACrBI,kDAAcH,UAAd;AACAI,iDAAaH,QAAb;AACAnC;AACH;AACJ,6BANY,EAMX,GANW,CAAb;AAOAmC,uCAAW7C,WAAW,YAAU;AAC5B,oCAAI;AACA+C,kDAAcH,UAAd;AACH,iCAFD,CAEC,OAAO7B,CAAP,EAAS,CAAE;AACf,6BAJU,EAIT,IAJS,CAAX;AAKH,yBAfD,MAeK;AACDL;AACH;AACJ;AAhFI;AAbO,aAAT,CAAX;AAgGH;;;;EArGkBuC,I","file":"launcher.js","sourcesContent":["\r\nclass launcher extends Page{\r\n    constructor(hash){\r\n        super(hash);\r\n    }\r\n    vue_(){\r\n        this.vue = new VueP({\r\n            el: this.hash+'_vue',\r\n            created() {},\r\n            compiled(){\r\n                this.init();\r\n                setTimeout(function(){\r\n                    window.launcher_hasCompiled = true;\r\n                },300);\r\n            },\r\n            data: {\r\n                hasPosition: false\r\n            },\r\n            computed: {},\r\n            methods: {\r\n                navChoose(obj,index,func=null){\r\n                    let i,length=obj.length;\r\n                    for(i=0;i<length;i++){\r\n                        if(i===index){\r\n                            obj[i].choose = true;\r\n                        }else{\r\n                            obj[i].choose = false;\r\n                        }\r\n                    }\r\n                    if(func){\r\n                        func(index);\r\n                    }\r\n                },\r\n                handlerError(e){\r\n                    switch(e){\r\n                        case -8:\r\n                            Message.toast('发送过于频繁', 3);\r\n                            break;\r\n                        default:\r\n                            return false;\r\n                            break;\r\n                    }\r\n                    return true;\r\n                },\r\n                init() {},\r\n                jump2index(){\r\n                    console.log('5555555')\r\n                    if(app.secondHash === 'index'){\r\n                        Util.hasLoadJsCallBack('index', ()=>{\r\n                            console.log('3333')\r\n                            console.log(index)\r\n                            this.jump('index');\r\n                        });\r\n                    }\r\n                },\r\n                look(imgStr){\r\n                    var imgArr = imgStr.split('|');\r\n                    var urls = [];\r\n                    for(var i=0;i<imgArr.length;i++){\r\n                        var src = imgArr[i].trim();\r\n                        if(!!src){\r\n                            urls.push(src);\r\n                        }\r\n                    }\r\n\r\n                    if(urls.length <= 0){\r\n                        return;\r\n                    }else{\r\n                        wx.previewImage({\r\n                            current: '', // 当前显示图片的http链接\r\n                            urls: urls // 需要预览的图片http链接列表\r\n                        });\r\n                    }\r\n                },\r\n                showAndHideCover(obj_vue){\r\n                    obj_vue.find('.endtouch').show();\r\n                    setTimeout(function(){\r\n                        obj_vue.find('.endtouch').hide();\r\n                    },800);\r\n                },\r\n                smooth(func){\r\n                    if(app.isDoingAnimation){\r\n                        var interTimer;\r\n                        var outTimer;\r\n                        interTimer = setInterval(function(){\r\n                            if(!app.isDoingAnimation){\r\n                                clearInterval(interTimer);\r\n                                clearTimeout(outTimer);\r\n                                func();\r\n                            }\r\n                        },100);\r\n                        outTimer = setTimeout(function(){\r\n                            try {\r\n                                clearInterval(interTimer);\r\n                            }catch (e){}\r\n                        },2000);\r\n                    }else{\r\n                        func();\r\n                    }\r\n                }\r\n            }\r\n        });\r\n    }\r\n}"]}