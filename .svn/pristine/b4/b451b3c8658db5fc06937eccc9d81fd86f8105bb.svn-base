.switch{height:34px;background:#ddd;width:67px;border-radius:46px;transition:all .2s ease}
.switch label{position:absolute;width:30px;height:30px;background:#fff;margin-top:2px;border-radius:50%;-webkit-transform:translateX(2px);transition:all .2s ease}
.switch input{display:none}
.switch.open{background-color:#0088e4}
.switch.open label{-webkit-transform:translateX(35px)}
select{background-color:#fff;border:0;text-align:right;outline:0}
blockquote,q{quotes:none}
blockquote:after,blockquote:before,q:after,q:before{content:'';content:none}
