data) => {\r\n                        if(data.e !== 0){\r\n                            Message.toast(\"请输入手机号\")\r\n                        }else{\r\n                            if(data.data.code){\r\n                                this.phone = data.data.code;\r\n                            }else{\r\n                                Message.toast(\"查不到相关信息\")\r\n                            }\r\n                        }\r\n                    });\r\n                },\r\n                init(){\r\n\r\n                }\r\n            }\r\n        });\r\n    }\r\n}"]}